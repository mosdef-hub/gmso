<ForceField version="0.0.1" name="LennardJones-FENE">
    <FFMetaData combiningRule="lorentz">
        <Units energy="energy" distance="length" mass="amu" charge="charge"/>
    </FFMetaData>
    <AtomTypes>
       <AtomType name="A" element="A" atomclass="A" mass="1.0" charge="0.0" definition="A" description="CG Bead">
       </AtomType>
    </AtomTypes>
    <BondTypes expression="-0.5 * K * R0**2 * log(1-(r/R0)**2) + 4 * epsilon * ((sigma/r)**12 - (sigma/r)**6) + epsilon">
        <ParametersUnitDef parameter="R0" unit="length"/>
        <ParametersUnitDef parameter="K" unit="energy/distance^2"/>
        <ParametersUnitDef parameter="sigma" unit="length"/>
        <ParametersUnitDef parameter="epsilon" unit="energy"/>
        <BondType name="FENE(A-A)" type1='A' type2='A'>
            <Parameters>
                <Parameter name='R0' value="1.0"/>
                <Parameter name='K' value="10"/>
                <Parameter name='sigma' value="1.0"/>
                <Parameter name='epsilon' value="1.0"/>
            </Parameters>
        </BondType>
    </BondTypes>
  <PairPotentialTypes expression="4 * epsilon * ((sigma/r)**12 - (sigma/r)**6)">
    <ParametersUnitDef parameter="sigma" unit="length"/>
    <ParametersUnitDef parameter="epsilon" unit="energy"/>
    <ParametersUnitDef parameter="r_cut" unit="length"/>
    <PairPotentialType name="LJ(A:A)" type1="A" type2="A">
        <Parameters>
            <Parameter name="epsilon" value="1.0"/>
            <Parameter name="sigma" value="1.0"/>
        </Parameters>
    </PairPotentialType>
  </PairPotentialTypes>
</ForceField>
